// Test 1.4: Complex Boolean Expressions
machine "Complex Conditions" {
    count: 15;
    enabled: true;
    status: "ready";
}

state Start @start
state PathA
state PathB
state PathC

Start -when: "count > 10 && enabled == true"-> PathA
Start -when: "status == 'ready' || status == 'pending'"-> PathB
Start -unless: "count < 5"-> PathC
