machine "Example"

task init {
    query<string>:;
}

task analyse {
    prompt: "This is my prompt for analyse";
}

tool feedback {
    retry<int>: 4;
}

complete;

tool another;

data query {
    value<string>:;
}

data result {
    value<string>:;
}

init -> analyse;
analyse -next: 'asdasd';-> another;
analyse -on: foo;-> feedback;
feedback =catch: 'method(4)';=> complete;
init -read: 'input.foo';-> query;
analyse - update -> result;
complete - read -> result;
